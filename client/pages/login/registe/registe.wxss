/* pages/login/registe/registe.wxss */
#register{
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  
}
.rows{
  width: 90%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
  margin-top: 20rpx;
}
.rows label{
  font-size: 30rpx;
  color: gray;
  display: inline-block;
  width: 200rpx;
}
.rows input{
  width: 60%;
  height: 60%;
  border-bottom: 1px solid gray;
  border-radius: 5rpx;
  margin-left: 20rpx;
}
#button{
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
#button button{
  width: 300rpx;
  height: 70rpx;
  line-height: 70rpx;
  margin-top: 50rpx;
}